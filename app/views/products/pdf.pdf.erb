<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Title</th>
          <th>Description</th>
          <th>Url image</th>
          <th>Price</th>
        </tr>
      </thead>
      <tbody>
        <h1>Hola mundo</h1>
        <% @products.each do |product| %>
        <tr>
          <td><%= product.title %></td>
          <td><%= product.description %></td>
          <td><%= product.image_url %></td>
          <td><%= product.price %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </body>
</html>
